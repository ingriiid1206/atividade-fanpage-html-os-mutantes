  <!-- SLIDESHOW SCRIPT -->
  <script>
    let slideIndex = 1;
    showSlides(slideIndex);

    function plusSlides(n) {
      showSlides(slideIndex += n);
    }

    function showSlides(n) {
      let i;
      let slides = document.getElementsByClassName("mySlides");
      if (n > slides.length) {slideIndex = 1}
      if (n < 1) {slideIndex = slides.length}
      for (i = 0; i < slides.length; i++) {
        slides[i].style.display = "none";
      }
      slides[slideIndex-1].style.display = "block";
    }
  </script>

  <!-- YOUTUBE PLAYER + LETRA -->
  <script>
    let player;
    let currentIndex = -1;
    const lyrics = [
      { time: 2, text: "♫⋆｡♪  𝄞 ₊˚ ‧ ⊹" },
      { time: 12, text: "  𝄞 ₊˚ ‧ ♪  ˚ ⊹" }, 
      { time: 22, text: "♫₊ ♪ *♬‧₊" },  
      { time: 29, text: "♫⋆Eu quis cantar, minha canção iluminada de sol｡♪" },
      { time: 33, text: "♫⋆Soltei os panos sobre os mastros no ar｡♪" },
      { time: 38, text: "♫⋆Mas as pessoas na sala de jantar｡♪" },
      { time: 47, text: "♫⋆Essas pessoas na sala de jantar｡♪" },
      { time: 50, text: "♫⋆São ocupadas em nascer e morrer｡♪" },

      
    ];

    function onYouTubeIframeAPIReady() {
      player = new YT.Player('player', {
        height: '360',
        width: '640',
        videoId: 'gfs9DC4GNr0',
        events: {
          'onReady': startLyricsSync
        }
      });
    }

    function startLyricsSync() {
      setInterval(() => {
        const currentTime = player.getCurrentTime();
        for (let i = lyrics.length - 1; i >= 0; i--) {
          if (currentTime >= lyrics[i].time) {
            if (currentIndex !== i) {
              document.getElementById('lyrics').innerText = lyrics[i].text;
              currentIndex = i;
            }
            break;
          }
        }
      }, 500);
    }

    var tag = document.createElement('script');
    tag.src = "https://www.youtube.com/iframe_api";
    var firstScriptTag = document.getElementsByTagName('script')[0];
    firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
  </script>

